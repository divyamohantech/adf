{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "RemoveDublicate"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: false,",
				"     validateSchema: true) ~> source1",
				"source1 select(mapColumn(",
				"          COLOR_TOOL_NAME = Color_Tool,",
				"          NAME = Color_Chip_Name,",
				"          CREATE_DATE = DateTime,",
				"          CHIP_SOURCE = Color_Chip_Source,",
				"          MEASUREMENT_SCHEME = Scheme,",
				"          Instrument,",
				"          Geometry,",
				"          i400,",
				"          i410,",
				"          i420,",
				"          i430,",
				"          i440,",
				"          i450,",
				"          i460,",
				"          i470,",
				"          i480,",
				"          i490,",
				"          i500,",
				"          i510,",
				"          i520,",
				"          i530,",
				"          i5Color1,",
				"          i550,",
				"          i560,",
				"          i570,",
				"          i580,",
				"          i590,",
				"          i600,",
				"          i610,",
				"          i620,",
				"          i630,",
				"          i640,",
				"          i650,",
				"          i660,",
				"          i670,",
				"          i680,",
				"          i690,",
				"          i700",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 aggregate(groupBy(COLOR_TOOL_NAME),",
				"     CHIP_SOURCE = ERROR_FUNCTION('')) ~> RemoveDublicate"
			]
		}
	}
}